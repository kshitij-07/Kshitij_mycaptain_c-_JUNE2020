// (Q)  Write a C++ program to access the elements of an array using pointers.

#include<iostream>
using namespace std;
int main()
{
	int n[5],i;
	cout<<"ENTER THE NUMBERS"<<endl;
	for(i=1;i<=5;i++)
	{
		cin>>n[i];
	}
	cout<<"YOU HAVE ENTERED"<<endl;
	for(i=1;i<=5;i++)
	{
		cout<<*(n+i)<<endl;
	}
	return 0;
}
